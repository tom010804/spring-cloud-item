<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jk.dao.UserDao">

    <insert id="addOneUser" keyProperty="userId" useGeneratedKeys="true">
        insert into t_user(userPhone,shopName,shopLocation,shopLogo) values (#{userPhone},#{shopName},#{shopLocation},#{shopLogo})
    </insert>

</mapper>